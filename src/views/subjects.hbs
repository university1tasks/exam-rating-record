<html>
  <head>
    <title>Список предметов</title>
    <meta charset="utf-8" />
  </head>
  <body>
    <a href="/subjects/create">Добавить предмет</a>
    <h1>Список предметов</h1>
    <table>
      <tr><th>Название</th><th>Преподаватель</th></tr>
      {{#each subjects}}
        <tr><td>{{this.name}}</td><td>{{this.teacher.lastName}}
            {{this.teacher.firstName}}
            {{this.teacher.patronymic}}</td></tr>
      {{/each}}
    </table>
  </body>
</html>